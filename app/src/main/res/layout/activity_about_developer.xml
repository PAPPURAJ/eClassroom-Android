<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2196F3"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".AboutDeveloper">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/info_top"
        android:scaleType="fitXY"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/info_bottom"
        android:scaleType="fitXY"/>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginRight="40dp"
        android:layout_marginLeft="40dp"
        android:orientation="vertical">


        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/about_pappu"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/contact_pappurajCallTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/call_icon"/>

                        <ImageView
                            android:id="@+id/contact_pappurajMailTv"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/gmail_icon"/>

                        <ImageView
                            android:id="@+id/contact_pappurajLinkedinTv"
                            android:layout_width="27dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/linkedin_icon"/>

                        <ImageView
                            android:id="@+id/contact_pappurajFbTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/fb_icon"/>

            </LinearLayout>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>



        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/about_nayeem"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/contact_nayeemCallTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/call_icon"/>

                        <ImageView
                            android:id="@+id/contact_nayeemMailTv"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/gmail_icon"/>

                        <ImageView
                            android:id="@+id/contact_nayeemLinkedinTv"
                            android:layout_width="27dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/linkedin_icon"/>

                        <ImageView
                            android:id="@+id/contact_nayeemFbTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/fb_icon"/>

            </LinearLayout>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>




        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/about_yamin"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/contact_yaminCallTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/call_icon"/>

                        <ImageView
                            android:id="@+id/contact_yaminMailTv"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/gmail_icon"/>

                        <ImageView
                            android:id="@+id/contact_yaminLinkedinTv"
                            android:layout_width="27dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/linkedin_icon"/>

                        <ImageView
                            android:id="@+id/contact_yaminFbTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/fb_icon"/>

            </LinearLayout>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>




        <androidx.coordinatorlayout.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/about_nabil"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="15dp"
                android:layout_marginLeft="15dp"
                android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/contact_nabilCallTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/call_icon"/>

                        <ImageView
                            android:id="@+id/contact_nabilMailTv"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/gmail_icon"/>

                        <ImageView
                            android:id="@+id/contact_nabilLinkedinTv"
                            android:layout_width="27dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/linkedin_icon"/>

                        <ImageView
                            android:id="@+id/contact_nabilFbTv"
                            android:layout_width="25dp"
                            android:layout_height="30dp"
                            android:layout_margin="10dp"
                            android:src="@drawable/fb_icon"/>

            </LinearLayout>

        </androidx.coordinatorlayout.widget.CoordinatorLayout>










    </LinearLayout>

    </ScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>